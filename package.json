{
    "name": "deakin-detonator-toolkit",
    "private": true,
    "version": "0.1.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "tauri": "tauri",
        "style": "prettier --check .",
        "style:fix": "prettier --write .",
        "start": "vite",
        "prepare": "husky install",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "update": "node cd_scripts/update.mjs"
