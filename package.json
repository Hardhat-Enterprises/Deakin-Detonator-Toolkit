    "dependencies": {
        "@actions/github": "^5.1.1",
        "@emotion/react": "^11.10.4",
        "@mantine/core": "^5.3.0",
        "@mantine/form": "^5.3.0",
        "@mantine/hooks": "^5.3.0",
        "@mantine/notifications": "5.3.0",
        "@mantine/prism": "^5.3.0",
        "@mantine/spotlight": "^5.3.0",
        "@tabler/icons": "^1.95.1",
        "@tauri-apps/api": "^1.0.2",
        "css-loader": "^7.1.2",
        "esbuild": "^0.23.0",
        "node-fetch": "^3.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "6.3.0",
        "style-loader": "^4.0.0",
        "tsx": "^4.17.0"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^1.5.6",
