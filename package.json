        "@types/react-dom": "^18.0.6",
        "@vitejs/plugin-react": "^2.2.0",
        "husky": "^8.0.1",
        "jsdom": "^20.0.0",
        "prettier": "2.7.1",
        "pretty-quick": "^3.1.3",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5",
        "vite": "^3.2.10",
        "vitest": "^0.23.2"
    }
}
